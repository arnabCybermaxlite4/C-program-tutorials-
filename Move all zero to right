#include <stdio.h>

void moveZerosToRight(int arr[], int n) {
    int count = 0; // Position for non-zero elements from left
    int i;

    // Traverse the array
    for (i = 0; i < n; i++) {
        if (arr[i] != 0) {
            arr[count] = arr[i]; // Place non-zero elements in front
            count++;
        }
    }

    // Fill remaining places with 0
    while (count < n) {
        arr[count] = 0;
        count++;
    }
}

int main() {
    int arr[100], n, i;

    printf("Enter size of array: ");
    scanf("%d", &n);

    printf("Enter elements of array:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    moveZerosToRight(arr, n);

    printf("Array after moving zeros to right:\n");
    for (i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
